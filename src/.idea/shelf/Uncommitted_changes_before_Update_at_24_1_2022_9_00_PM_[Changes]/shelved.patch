<+><!--xu yong lin-->\r\n{% extends \"admin/admin_base.html\" %}\r\n{% block title %}Admin Display Certification{% endblock %}\r\n\r\n{% block head %}\r\n    <style>\r\n        .btn {\r\n            margin-left: 10px;\r\n            margin-right: 10px;\r\n        }\r\n        #infodisplay{\r\n            width: 90%;\r\n            margin: auto;\r\n        }\r\n        .certdisplay {\r\n            width: 25%;\r\n        }\r\n\r\n        .modal {\r\n            font-color: black;\r\n            font-weight: normal;\r\n        }\r\n\r\n        #deletedisplay th {\r\n            width: 30%;\r\n            overflow: visible;\r\n        }\r\n\r\n        #deletedisplay td {\r\n            width: 70%;\r\n            overflow: visible;\r\n        }\r\n\r\n    </style>\r\n{% endblock %}\r\n\r\n{% block content %}\r\n    <div class=\"\">\r\n        <h1>EXTENSION TO RESTAURANT DETAILS</h1>\r\n        <div>\r\n            {# hygiene cert #}\r\n            <table class=\"table\" id=\"infodisplay\"\r\n                   style=\"background-color: white\">\r\n                <thead>\r\n                {% for cert in certificate_list %}\r\n                    <tr style=\"background-color: rgba(175, 97, 203,0.4)\">\r\n                        <th scope=\"col\" colspan=\"2\">\r\n                            {{ cert.id }}'s Certificates\r\n                        </th>\r\n                        <th style=\"width:300px; display: flex; float:right; margin-right: -80px;\">\r\n                            <a href=\"/admin/updateCertification/{{ cert.id }}\" class=\"btn btn-danger\">Update</a>\r\n                            <form action=\"{{ url_for('delete_cert', id=cert.id) }}\"\r\n                                  method=\"POST\">\r\n                                <button type=\"button\" class=\"btn btn-danger\" data-bs-toggle=\"modal\"\r\n                                        data-bs-target=\"#myModal_{{ cert.id }}\">\r\n                                    Delete\r\n                                </button>\r\n                                <!-- The Modal -->\r\n                                <div class=\"modal\" id=\"myModal_{{ cert.id }}\">\r\n                                    <div class=\"modal-dialog modal-dialog-centered modal-lg\">\r\n                                        <div class=\"modal-content\">\r\n                                            <!-- Modal Header -->\r\n                                            <div class=\"modal-header\">\r\n                                                <h4 class=\"modal-title\">Delete Confirmation</h4>\r\n                                                <button type=\"button\" class=\"btn-close\"\r\n                                                        data-bs-dismiss=\"modal\"></button>\r\n                                            </div>\r\n                                            <!-- Modal body -->\r\n                                            <div class=\"modal-body\">\r\n                                                Are you sure you want to\r\n                                                delete\r\n                                                <bold>ALL</bold>\r\n                                                the certification of {{ cert.id }}?\r\n                                                <hr>\r\n                                                <p>Overview of {{ cert.id }}'s Certifications:\r\n                                                <table class=\"table\" id=\"deletedisplay\">\r\n                                                    <thead>\r\n                                                    <tr>\r\n                                                        <th scope=\"col\">Certificate Name</th>\r\n                                                        <th scope=\"col\">File name</th>\r\n                                                    </tr>\r\n                                                    </thead>\r\n                                                    <tbody>\r\n                                                    <tr>\r\n                                                        <th scope=\"row\">Hygiene Certificate</th>\r\n                                                        <td>{{ cert.hygiene_cert }}</td>\r\n                                                    </tr>\r\n                                                    <tr>\r\n                                                        <th scope=\"row\">Halal Certificate</th>\r\n                                                        {% if not cert.halal_cert %}\r\n                                                            <td>NIL</td>\r\n                                                        {% else %}\r\n                                                            <td>{{ cert.halal_cert }}</td>\r\n                                                        {% endif %}\r\n                                                    </tr>\r\n                                                    <tr>\r\n                                                        <th scope=\"row\">Vegetarian Certificate</th>\r\n                                                        {% if not cert.vegetarian_cert %}\r\n                                                            <td>NIL</td>\r\n                                                        {% else %}\r\n                                                            <td>{{ cert.vegetarian_cert }}</td>\r\n                                                        {% endif %}\r\n                                                    </tr>\r\n                                                    <tr>\r\n                                                        <th scope=\"row\">Vegan Certificate</th>\r\n                                                        {% if not cert.vegan_cert %}\r\n                                                            <td>NIL</td>\r\n                                                        {% else %}\r\n                                                            <td>{{ cert.vegan_cert }}</td>\r\n                                                        {% endif %}\r\n                                                    </tr>\r\n                                                    <tr>\r\n                                                        <th scope=\"row\">No Pork No lard</th>\r\n                                                        <td>{{ cert.noPorknoLard }}</td>\r\n                                                    </tr>\r\n                                                    <tr>\r\n                                                        <th scope=\"row\">No Beef</th>\r\n                                                        <td>{{ cert.noBeef }}</td>\r\n                                                    </tr>\r\n                                                    </tbody>\r\n                                                </table>\r\n                                                </p>\r\n                                            </div>\r\n\r\n                                            <!-- Modal footer -->\r\n                                            <div class=\"modal-footer\">\r\n                                                <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">\r\n                                                    Cancel\r\n                                                </button>\r\n                                                <input type=\"submit\" value=\"Delete\" class=\"btn btn-danger\"\r\n                                                       style=\"background-color: red !important;\">\r\n                                            </div>\r\n\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </form>\r\n                        </th>\r\n                    </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                    <tr>\r\n                        <td rowspan=\"6\" style=\"background-color:rgba(56, 183, 78,0.4); width:20%;\">{{ cert.id }}</td>\r\n                        <td class=\"certdisplay\">Hygiene</td>\r\n                        <td>\r\n{#                            <iframe src=\"application/static/restaurantCertification/hygiene/1/{{ cert.hygiene_cert }}\">Hygiene cert</iframe>#}\r\n                            <a href=\"{{ url_for(\"static\", filename=cert.hygiene_cert[19:]) }}\" target=\"_blank\">{{ cert.hygiene_cert[53:] }}</a>\r\n                        </td>\r\n                    </tr>\r\n                    <tr>\r\n                        <td class=\"certdisplay\">Halal</td>\r\n                        {% if cert.halal_cert == '' %}\r\n                            <td>NIL</td>\r\n                        {% else %}\r\n                            <td>\r\n                                <a href=\"{{ url_for(\"static\", filename=cert.halal_cert[filename]) }}\" target=\"_blank\">{{ cert.halal_cert }}</a>\r\n                            </td>\r\n                        {% endif %}\r\n                    </tr>\r\n                    <tr>\r\n                        <td class=\"certdisplay\">Vegetarian</td>\r\n                        {% if cert.vegetarian_cert == '' %}\r\n                            <td>NIL</td>\r\n                        {% else %}\r\n                            <td>\r\n                                <a href=\"{{ url_for(\"static\", filename=cert.vegetarian_cert['filename']) }}\" target=\"_blank\">Click me</a>\r\n                            </td>\r\n                        {% endif %}\r\n                    </tr>\r\n                    <tr>\r\n                        <td class=\"certdisplay\">No Pork No Lard</td>\r\n                        <td>{{ cert.noPorknoLard }}</td>\r\n                    </tr>\r\n                    <tr>\r\n                        <td class=\"certdisplay\">Vegan</td>\r\n                        {% if cert.vegan_cert == '' %}\r\n                            <td>NIL</td>\r\n                        {% else %}\r\n                            <td>\r\n                                <a href=\"{{ url_for(\"static\", filename=cert.vegan_cert['filename']) }}\" target=\"_blank\">Click me</a>\r\n                            </td>\r\n                        {% endif %}\r\n                    </tr>\r\n                    <tr>\r\n                        <td class=\"certdisplay\">No Beef</td>\r\n                        <td>{{ cert.noBeef }}</td>\r\n                    </tr>\r\n                {% endfor %}\r\n                </tbody>\r\n            </table>\r\n            {# halal cert #}\r\n            {# vegetarian cert #}\r\n            {# no pork no lard indication #}\r\n            {# vegan cert #}\r\n            {# no beef indication #}\r\n        </div>\r\n    </div>\r\n\r\n{% endblock %}
